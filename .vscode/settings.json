{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "out-tsc": true,
        "**/*.js": { "when": "$(basename).ts" },
        "**/*.js.map": { "when": "$(basename)" }
    }
}